{
    "manifest_version": 2,
    "name": "Facebook Feeds Eraser",
    "description": "Remove the feeds from your Facebook page.",
    "version": "1.0",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "run_at": "document_start"
        }
    ]
}
